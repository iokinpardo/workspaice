{
  "manifest_version": 2,
  "name": "Parental Control Extension",
  "version": "1.0",
  "description": "A Firefox extension for implementing parental controls to monitor and restrict web browsing activities",
  "icons": {
    "48": "icons/icon-48.png"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
    "storage"
  ],
  "background": {
    "scripts": ["scripts/background.js"]
  },
  "browser_action": {
    "default_icon": "icons/icon-48.png",
    "default_title": "Parental Control"
  }
}